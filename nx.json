{
  "npmScope": "nx-react-simple-workspace",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "hooks-get-started": {
      "tags": []
    },
    "hooks-get-started-e2e": {
      "tags": [],
      "implicitDependencies": ["hooks-get-started"]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
